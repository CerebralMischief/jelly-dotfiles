"" ==================== Settings ==================== "
loadplugins
set guioptions=Cs
set titlestring=Firefox
set history=900
set showmode
set activate=quickmark,paste
set editor="gvim -f"
set followhints=0
set newtab=all
set visualbell
"set complete=ltSsf
set wildmode=list:longest,full
autocmd LocationChange ^https?://(docs|mail)\.google\.com/ :normal! <C-z>
autocmd LocationChange ^http://www\.google\.com/reader/ :normal! <C-z>
set middlemouse.contentLoadURL=false
set middlemouse.paste=false
